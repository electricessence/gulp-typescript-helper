{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny":true,
    "strictNullChecks": true,
    "removeComments":true,
    "declaration": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "typings",
    "tests",
    "node_modules",
    "bower_components",
    "dist",
    ".nuget",
    "documentation"
  ]
}
